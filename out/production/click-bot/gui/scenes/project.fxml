<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.ProjectController">
   <children>
      <TreeView fx:id="programTree" cacheShape="false" fixedCellSize="25.0" layoutX="204.0" prefHeight="400.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <TableView fx:id="variableList" layoutY="250.0" mouseTransparent="true" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="400.0" AnchorPane.topAnchor="220.0">
        <columns>
          <TableColumn fx:id="nameVariableList" editable="false" prefWidth="120.0" sortable="false" text="Name" />
          <TableColumn fx:id="valueVariableList" editable="false" prefWidth="64.8" sortable="false" text="Value" />
        </columns>
      </TableView>
      <SplitMenuButton fx:id="addCommandButton" layoutX="40.0" layoutY="114.0" mnemonicParsing="false" onAction="#addCommandWindow" text="Add Command">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </SplitMenuButton>
      <SplitMenuButton fx:id="addVariableButton" layoutX="39.0" layoutY="162.0" mnemonicParsing="false" onAction="#addVariableWindow" prefHeight="26.0" prefWidth="121.0" text="Add Variable">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </SplitMenuButton>
      <Label fx:id="nameProgramLabel" alignment="CENTER" layoutY="31.0" prefHeight="34.0" prefWidth="200.0" text="Name of Program">
         <font>
            <Font name="Candara" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
